                    featureEditor: '',
                    activeName: 'first',
                    loading: false,
                    dialogVisible: this.visible,
                    currProduct: {
                        packageList: [

                        ],
                        partsList: [

                        ],
                        partChildren: [],
                        fittingChildren: [],
                        status: 0,
                        cName: void 0,
                        fullCName: void 0,
                        eName: void 0,
